package test;

import java.util.Scanner;

public class test01 {
	public static void main(String[] args) {
		while(true) {
			Scanner sc = new Scanner(System.in);
			int num =sc.nextInt();
			switch(num) {
			case 1:
				System.out.println("1 실행");
				break;
			case 2:
				System.out.println("2 실행");
				break;
			}
		}
	}
}
