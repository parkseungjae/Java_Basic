package casting;

class A01{
	public void getTx() {
		System.out.println("A클래스");
	}
}
class B01 extends A01{
	public void getTx() {
		System.out.println("B클래스");
	}
}
class C01 extends A01{
	public void getTx() {
		System.out.println("C클래스");
	}
}

public class Ex01 {
	public static void main(String[] args) {
		/*
		 * up casting
		 * - 자식 형태의 값을 부모 형태로 변환하는 것
		 * down casting
		 * - 부모형태에서 자식형태로 변환하는것
		 */
		C01 c1 = new C01();
	}
}
